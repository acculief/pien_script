{
  "name": "ぴえん🥺スクリプト",
  "version": "0.0.0.2",
  "manifest_version": 2,
  "description": "ネガティブな言葉を🥺に変換する拡張機能",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery-3.5.1.min.js", "index.js"],
      "run_at": "document_idle"
    }
  ],
  "browser_action": {
    "default_icon": "pien-logo.png"
  }
}
